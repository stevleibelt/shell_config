  source $PATH_SHELL_CONFIG'/function.local'
fi
source $PATH_SHELL_CONFIG'/alias'
if [ -f $PATH_SHELL_CONFIG'/alias.local' ]; then
  source $PATH_SHELL_CONFIG'/alias.local'
fi
source $PATH_SHELL_CONFIG'/setting'
