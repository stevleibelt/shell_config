#.
alias ..='cd ..';

#a
alias apacheRestart='sudo systemctl restart httpd.service';
alias apacheStart='sudo systemctl start httpd.service';
alias apacheStop='sudo systemctl start httpd.service';

#c
alias cal='cal -3';
alias cd=net_bazzline_cd;
alias cdConfig='cd '$HOME'/config';
alias cdGitHub='cd '$HOME'/data/git/com/github';
alias cdPhp='cd '$HOME'/code/php';

#g
alias gitPushToMaster='git push -u origin master';

#l
alias la='ls -halt';

#m
alias mkdir=net_bazzline_mkdir;
alias mysqlRestart='sudo systemctl restart mysql.service';
alias mysqlStart='sudo systemctl start mysql.service';
alias mysqlStop='sudo systemctl start mysql.service';

#p
alias psGrep=net_bazzline_psgrep;

#s
if [ -f $HOME'/code/sh/local/displayBatteryStatus.sh' ]; then
  alias statusBattery='sh '$HOME'/code/sh/local/displayBatteryStatus.sh';
fi
if [ -f $HOME'/code/sh/local/sensorsRefresh.sh' ]; then
  alias statusSensor='sh '$HOME'/code/sh/local/sensorsRefresh.sh';
fi
alias system='net_bazzline_system_maintenance';

#w
alias webRestart='apacheRestart mysqlRestart';
alias webStart='apacheStart mysqlStart';
alias webStop='apacheStop mysqpStop';
